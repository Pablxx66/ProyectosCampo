<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.llokallasoftware.proyectoscampo.Vista.Fragmentos.DatosCalculo">

    <!-- TODO: Update blank fragment layout -->


    <LinearLayout
        android:id="@+id/layLinearV"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imgDatos"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:contentDescription="@string/descImgCalculos"
            android:src="@drawable/pruebacanal" />

        <LinearLayout
